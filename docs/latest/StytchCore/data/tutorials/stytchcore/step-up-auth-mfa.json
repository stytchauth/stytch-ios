ard let session = StytchClient.sessions.session else { return }","","        if session.authenticationFactors.contains(where: { $0.kind == \"sms\" && Date() <= $0.lastAuthenticatedAt.addingTimeInterval(60 * 5) }) {","            present(EditSettingsViewController(), animated: true)","        } else {","            Task {","                do {","                    let response = try await StytchClient.otps.send(parameters: .init(deliveryMethod: .sms(phoneNumber: user.phoneNumberE164)))","                    let controller = OTPCodeVerificationViewController(methodId: response.methodId, session: session) { [weak self] _ in","                        self?.navigationController?.popToViewController(self, animated: true)","                        present(EditSettingsViewController(), animated: true)","                    }","                    navigationController?.pushViewController(controller, animated: true)","                } catch {}","            }","        }","    }","}"],"fileName":"UserSettingsViewController.swift","fileType":"swift","identifier":"01-step-up-auth-mfa-06.swift","syntax":"swift","type":"file","highlights":[{"line":22},{"line":23},{"line":24},{"line":25},{"line":26}]},"https://stytch.com/docs/api/authenticate-otp":{"identifier":"https:\/\/stytch.com\/docs\/api\/authenticate-otp","titleInlineContent":[{"text":"authenticate","type":"text"}],"title":"authenticate","url":"https:\/\/stytch.com\/docs\/api\/authenticate-otp","type":"link"}}}