e":{"navigatorTitle":[{"text":"RBACPolicyRole","kind":"identifier"}],"type":"topic","role":"symbol","url":"\/documentation\/stytchcore\/rbacpolicyrole","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"RBACPolicyRole","kind":"identifier"}],"title":"RBACPolicyRole","abstract":[{"text":"A Role is a named collection of permissions that links actions to a Resource.","type":"text"},{"text":" ","type":"text"},{"text":"Roles are assigned to Members, either explicitly by direct assignment or implicitly by matching attributes or conditions, which grants them permissions.","type":"text"},{"text":" ","type":"text"},{"text":"Role assignment can be programmatically managed through certain Stytch API endpoints.","type":"text"},{"text":" ","type":"text"},{"text":"Refer to this guide for details on controls for delegating Roles to Members (https:\/\/stytch.com\/docs\/b2b\/guides\/rbac\/role-assignment).","type":"text"},{"text":" ","type":"text"},{"text":"All Roles are stored in your Project’s RBAC Policy. You can create, manage, and assign Roles in the Dashboard (https:\/\/stytch.com\/dashboard).","type":"text"},{"text":" ","type":"text"},{"text":"Check out the RBAC overview to learn more about Stytch’s RBAC permissioning model (https:\/\/stytch.com\/docs\/b2b\/guides\/rbac\/overview).","type":"text"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/RBACPolicyRole","kind":"symbol"},"doc://stytch-ios.StytchCore/documentation/StytchCore":{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore","kind":"symbol","role":"collection","type":"topic","url":"\/documentation\/stytchcore","abstract":[{"type":"text","text":"Provides native iOS access to the Stytch SDK methods for ultimate flexibility."}],"title":"StytchCore"}}}