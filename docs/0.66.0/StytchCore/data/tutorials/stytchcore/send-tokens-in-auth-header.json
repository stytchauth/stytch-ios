l-setup":{"title":"Initial setup","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","type":"reference","isActive":true},{"text":".","type":"text"}],"estimatedTime":"5min","url":"\/tutorials\/stytchcore\/initial-setup","role":"project","kind":"project","type":"topic","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup"},"01-send-tokens-in-auth-header-06.swift":{"content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = {","            var headers: [String: String] = [:]","            if let token = StytchClient.sessions.sessionToken {","                headers[\"Authorization\"] = \"Bearer \\(token)\"","            }","            return headers","        }","        return true","    }","}"],"fileType":"swift","highlights":[{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19}],"fileName":"AppDelegate.swift","type":"file","syntax":"swift","identifier":"01-send-tokens-in-auth-header-06.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually","title":"Manage Sessions manually","type":"topic","role":"project","abstract":[{"type":"text","text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation."}],"kind":"project","url":"\/tutorials\/stytchcore\/manage-sessions-manually","estimatedTime":"5min"},"01-send-tokens-in-auth-header-05.swift":{"content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = { [:] }","        return true","    }","}"],"fileType":"swift","fileName":"AppDelegate.swift","type":"file","syntax":"swift","identifier":"01-send-tokens-in-auth-header-05.swift","highlights":[{"line":13}]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user":{"title":"Manage the current user","abstract":[{"type":"text","text":"This tutorial will teach you a few of the things you can do to manage Stytch users."}],"estimatedTime":"10min","url":"\/tutorials\/stytchcore\/manage-the-current-user","role":"project","kind":"project","type":"topic","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oauth#Add-Third-Party-Identity-Providers":{"title":"Add Third Party Identity Providers","abstract":[{"type":"text","text":"Using OAuth, or social logins, are a familiar and low-friction way to authenticate your users. Learn how to add them in this tutorial!"}],"url":"\/tutorials\/stytchcore\/add-oauth#Add-Third-Party-Identity-Providers","role":"pseudoSymbol","kind":"section","type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-oauth#Add-Third-Party-Identity-Providers"},"01-send-tokens-in-auth-header-02.swift":{"content":["import Foundation","","final class NetworkingClient {","    var headerProvider: (() -> [String: String])?","","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"fileType":"swift","highlights":[{"line":4},{"line":5}],"fileName":"NetworkingClient.swift","type":"file","syntax":"swift","identifier":"01-send-tokens-in-auth-header-02.swift"},"01-send-tokens-in-auth-header-03.swift":{"content":["import Foundation","","final class NetworkingClient {","    var headerProvider: (() -> [String: String])?","","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        headerProvider?().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"fileType":"swift","fileName":"NetworkingClient.swift","type":"file","syntax":"swift","identifier":"01-send-tokens-in-auth-header-03.swift","highlights":[{"line":18},{"line":19},{"line":20},{"line":21}]},"doc://st