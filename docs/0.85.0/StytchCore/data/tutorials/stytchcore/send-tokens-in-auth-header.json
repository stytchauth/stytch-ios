et session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"identifier":"01-send-tokens-in-auth-header-02.swift","fileName":"NetworkingClient.swift","syntax":"swift","type":"file","highlights":[{"line":4},{"line":5}],"fileType":"swift"},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Using-more-auth-methods":{"abstract":[],"title":"Using more auth methods","kind":"article","type":"topic","url":"\/tutorials\/stytch\/stytch-basics\/using-more-auth-methods","role":"article","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Stytch-Basics\/Using-more-auth-methods"}}}