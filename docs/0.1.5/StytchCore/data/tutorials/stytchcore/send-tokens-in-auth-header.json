ference"},{"text":".","type":"text"}],"title":"Initial setup","url":"\/tutorials\/stytchcore\/initial-setup","type":"topic","kind":"project","role":"project","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup"},"01-send-tokens-in-auth-header-01.swift":{"syntax":"swift","type":"file","fileType":"swift","fileName":"NetworkingClient.swift","highlights":[],"identifier":"01-send-tokens-in-auth-header-01.swift","content":["import Foundation","","final class NetworkingClient {","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"]}}}