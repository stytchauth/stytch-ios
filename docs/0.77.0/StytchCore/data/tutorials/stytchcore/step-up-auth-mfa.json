tton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        editSettingsButton.addTarget(self, action: #selector(didTapEditSettingsButton(sender:)), for: [.touchUpInside])","    }","","    @objc private func didTapEditSettingsButton(sender: UIButton) {","        guard let session = StytchClient.sessions.session else { return }","","        if session.authenticationFactors.contains(where: { $0.kind == \"sms\" && Date() <= $0.lastAuthenticatedAt.addingTimeInterval(60 * 5) }) {","            present(EditSettingsViewController(), animated: true)","        } else {","            Task {","                do {","                    let response = try await StytchClient.otps.send(parameters: .init(deliveryMethod: .sms(phoneNumber: user.phoneNumberE164)))","                    let controller = OTPCodeVerificationViewController(methodId: response.methodId, session: session) { [weak self] _ in","                        self?.navigationController?.popToViewController(self, animated: true)","                        present(EditSettingsViewController(), animated: true)","                    }","                    navigationController?.pushViewController(controller, animated: true)","                } catch {}","            }","        }","    }","}"],"identifier":"01-step-up-auth-mfa-06.swift","syntax":"swift","fileName":"UserSettingsViewController.swift","highlights":[{"line":22},{"line":23},{"line":24},{"line":25},{"line":26}]},"02-step-up-auth-mfa-06.swift":{"fileType":"swift","highlights":[{"line":28}],"fileName":"OTPCodeVerificationViewController.swift","content":["import StytchCore","import UIKit","","final class OTPVerificationViewController: UIViewController {","    private var methodId: String","","    private var onAuthenticate: (AuthenticateResponseType) -> Void","","    private var session: Session","","    private let submitButton: UIButton","","    private let codeTextField: UITextfield","","    override func viewDidLoad() {","        super.viewDidLoad()","","        codeTextField.textContentType = .oneTimeCode","        codeTextField.delegate = self","","        submitButton.addTarget(self, action: #selector(didSubmitCode), for: [.touchUpInside])","    }","","    @objc private func didSubmitCode() {","        Task {","            do {","                let authResponse = try await StytchClient.otps.authenticate(parameters: .init(code: codeTextField.text, methodId: methodId))","                onAuthenticate(authResponse)","            } catch {}","        }","    }","}","","extension OTPVerificationViewController: UITextFieldDelegate {","    func textFieldShouldReturn(_ textField: UITextField) -> Bool {","        if textField == codeTextField {","            didSubmitCode()","        }","        return true","    }","}"],"type":"file","syntax":"swift","identifier":"02-step-up-auth-mfa-06.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics#Register-and-authenticate-with-Biometrics":{"abstract":[{"text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!","type":"text"}],"type":"section","url":"\/tutorials\/stytchcore\/add-biometrics#Register-and-authenticate-with-Biometrics","kind":"section","role":"pseudoSymbol","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics#Register-and-authenticate-with-Biometrics","title":"Register and authenticate with Biometrics"}}}