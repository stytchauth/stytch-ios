lers.first(where: { $0.canHandle(url: url) }) else {","            return false","        }","        handler.handle(url: url)","        return true","    }","}"],"fileType":"swift","highlights":[{"line":38},{"line":39},{"line":40},{"line":41},{"line":42},{"line":43},{"line":44},{"line":45},{"line":46},{"line":47},{"line":48},{"line":49},{"line":50},{"line":51},{"line":52},{"line":53},{"line":54},{"line":55},{"line":56},{"line":57},{"line":58},{"line":59},{"line":60},{"line":61},{"line":62},{"line":63},{"line":64},{"line":65},{"line":66},{"line":67},{"line":68}],"identifier":"01-add-eml-04.swift","fileName":"AppDelegate.swift","type":"file","syntax":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually#Update-Session-tokens-using-updatesessionTokens":{"abstract":[{"text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation.","type":"text"}],"title":"Update Session tokens using update(sessionTokens:)","url":"\/tutorials\/stytchcore\/manage-sessions-manually#Update-Session-tokens-using-updatesessionTokens","role":"pseudoSymbol","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually#Update-Session-tokens-using-updatesessionTokens","kind":"section","type":"section"},"https://stytch.com/docs/api/log-in-or-create-user-by-email":{"url":"https:\/\/stytch.com\/docs\/api\/log-in-or-create-user-by-email","type":"link","title":"login_or_create","titleInlineContent":[{"type":"text","text":"login_or_create"}],"identifier":"https:\/\/stytch.com\/docs\/api\/log-in-or-create-user-by-email"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oauth#Add-Third-Party-Identity-Providers":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-oauth#Add-Third-Party-Identity-Providers","role":"pseudoSymbol","type":"section","title":"Add Third Party Identity Providers","kind":"section","abstract":[{"text":"Using OAuth, or