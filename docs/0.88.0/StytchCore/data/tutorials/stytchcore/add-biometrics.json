self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"fileName":"AuthenticationViewController.swift","fileType":"swift","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}],"identifier":"01-add-biometrics-06.swift","syntax":"swift","type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics#Register-and-authenticate-with-Biometrics":{"type":"section","kind":"section","url":"\/tutorials\/stytchcore\/add-biometrics#Register-and-authenticate-with-Biometrics","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics#Register-and-authenticate-with-Biometrics","abstract":[{"text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!","type":"text"}],"role":"pseudoSymbol","title":"Register and authenticate with Biometrics"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa":{"abstract":[{"text":"If your app has functionality which requires higher levels of security on certain screens vs others, you may want to consider using step-up authentication or MFA. This tutorial will teach you a simple way of doing this with Stytch.","type":"text"}],"role":"project","estimatedTime":"10min","kind":"project","url":"\/tutorials\/stytchcore\/step-up-auth-mfa","type":"topic","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/step-up-auth-mfa","title":"Step up auth\/MFA"}}}