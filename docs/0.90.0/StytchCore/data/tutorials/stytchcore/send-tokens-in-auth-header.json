   }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        headerProvider?().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"type":"file","identifier":"01-send-tokens-in-auth-header-03.swift","fileType":"swift","highlights":[{"line":18},{"line":19},{"line":20},{"line":21}]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually":{"url":"\/tutorials\/stytchcore\/manage-sessions-manually","kind":"project","type":"topic","role":"project","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually","title":"Manage Sessions manually","abstract":[{"type":"text","text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation."}],"estimatedTime":"5min"},"doc://stytch-ios.StytchCore/tutorials/stytch":{"url":"\/tutorials\/stytch","title":"Stytch Tutorials","role":"overview","kind":"overview","type":"topic","abstract":[{"text":"Go from zero to fully featured authentication in your iOS app powered by Stytch","type":"text"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually#Update-Session-tokens-using-updatesessionTokens":{"abstract":[{"type":"text","text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation."}],"title":"Update Session tokens using update(sessionTokens:)","type":"section","role":"pseudoSymbol","kind":"section","url":"\/tutorials\/stytchcore\/manage-sess