","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift","fileType":"swift","identifier":"01-add-biometrics-06.swift","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}]},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Getting-started":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Stytch-Basics\/Getting-started","title":"Getting started","type":"topic","role":"article","abstract":[],"kind":"article","url":"\/tutorials\/stytch\/stytch-