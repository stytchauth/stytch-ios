ocumentation\/stytchcore\/session","role":"symbol","kind":"symbol","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/Session","navigatorTitle":[{"kind":"identifier","text":"Session"}],"type":"topic","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Session"}]},"doc://stytch-ios.StytchCore/documentation/StytchCore":{"kind":"symbol","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore","url":"\/documentation\/stytchcore","type":"topic","title":"StytchCore","role":"collection","abstract":[{"type":"text","text":"Provides native iOS access to the Stytch SDK methods for ultimate flexibility."}]},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchObjectInfo":{"url":"\/documentation\/stytchcore\/stytchobjectinfo","role":"symbol","kind":"symbol","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchObjectInfo","title":"StytchObjectInfo","navigatorTitle":[{"text":"StytchObjectInfo","kind":"identifier"}],"fragments":[{"text":"enum","kind":"keyword"},{"text":" ","kind":"text"},{"text":"StytchObjectInfo","kind":"identifier"}],"abstract":[{"type":"text","text":"StytchObjectInfo provides a generic way to unify the publishing of Stytch object types."},{"type":"text","text":" "},{"type":"text","text":"If there is an object to publish you will get "},{"type":"codeVoice","code":"case available(T, Date)"},{"type":"text","text":", which will publish the object"},{"type":"text","text":" "},{"type":"text","text":"and the date is was last validated at. The receiver of the event if the object is within their time tolerance for use."},{"type":"text","text":" "},{"type":"text","text":"If there is no object to publish you will get "},{"type":"codeVoice","code":"case unavailable"},{"type":"text","text":". So the publisher will never have to publish a nil value."}],"type":"topic"}}}