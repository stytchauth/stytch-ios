on","","    private let submitButton: UIButton","","    private let codeTextField: UITextfield","","    override func viewDidLoad() {","        super.viewDidLoad()","","        codeTextField.textContentType = .oneTimeCode","    }","}"],"identifier":"02-step-up-auth-mfa-02.swift","syntax":"swift","highlights":[{"line":18}],"type":"file","fileType":"swift"},"02-step-up-auth-mfa-06.swift":{"fileName":"OTPCodeVerificationViewController.swift","highlights":[{"line":28}],"content":["import StytchCore","import UIKit","","final class OTPVerificationViewController: UIViewController {","    private var methodId: String","","    private var onAuthenticate: (AuthenticateResponseType) -> Void","","    private var session: Session","","    private let submitButton: UIButton","","    private let codeTextField: UITextfield","","    override func viewDidLoad() {","        super.viewDidLoad()","","        codeTextField.textContentType = .oneTimeCode","        codeTextField.delegate = self","","        submitButton.addTarget(self, action: #selector(didSubmitCode), for: [.touchUpInside])","    }","","    @objc private func didSubmitCode() {","        Task {","            do {","                let authResponse = try await StytchClient.otps.authenticate(parameters: .init(code: codeTextField.text, methodId: methodId))","                onAuthenticate(authResponse)","            } catch {}","        }","    }","}","","extension OTPVerificationViewController: UITextFieldDelegate {","    func textFieldShouldReturn(_ textField: UITextField) -> Bool {","        if textField == codeTextField {","            didSubmitCode()","        }","        return true","    }","}"],"type":"file","syntax":"swift","identifier":"02-step-up-auth-mfa-06.swift","fileType":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics":{"abstract":[{"text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!","type":"text"}],"title":