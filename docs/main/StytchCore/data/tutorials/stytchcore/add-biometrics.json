), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"type":"file","fileType":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml":{"type":"topic","url":"\/tutorials\/stytchcore\/add-eml","role":"project","title":"Add Email Magic Links","abstract":[{"text":"Email Magic Links are a great, user-friendly way to get started using Stytch!","type":"text"}],"estimatedTime":"10min","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml","kind":"project"},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Using-more-auth-methods":{"type":"topic","abstract":[],"role":"article","url":"\/tutorials\/stytch\/stytch-basics\/using-more-auth-methods","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Stytch-Basics\/Using-more-auth-methods","kind":"article","title":"Using more auth methods"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml#Send-an-EML":{"type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml#Send-an-EML","url":"\/tutorials\/stytchcore\/add-eml#Send-an-EML","kind":"section","role":"pseudoSymbol","abstract":[{"type":"text","text":"Email Magic Links are a great, user-friendly way to get started using Stytch!"}],"title":"Send an EML"},"d