Button) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"fileType":"swift","highlights":[{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30}]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"type":"section","kind":"section","title":"Configure the StytchClient","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient","abstract":[{"text":"This tutorial will walk you through the initial process for setting up the ","type":"text"},{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","type":"reference","isActive":true},{"type":"text","text":"."}],"role":"pseudoSymbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml#Set-up-deeplink-handling":{"title":"Set up deeplink handling","kind":"section","role":"pseudoSymbol","abstract":[{"type":"text","text":"Email Magic Links are a great, user-friendly way to get started using Stytch!"}],"type":"section","url":"\/tutorials\/stytchcore\/add-eml#Set-up-deeplink-handling","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml#Set-up-deeplink-handling"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Add-Stytch-to-your-project":{"kind":"section","title":"Add Stytch to your proj