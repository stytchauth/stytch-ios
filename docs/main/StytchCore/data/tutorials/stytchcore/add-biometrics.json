":"authenticate(parameters:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"text":"authenticate","kind":"identifier"},{"kind":"text","text":"("},{"kind":"externalParam","text":"parameters"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:10StytchCore0A6ClientV10BiometricsV22AuthenticateParametersV","text":"AuthenticateParameters"},{"kind":"text","text":") "},{"kind":"keyword","text":"async"},{"text":" ","kind":"text"},{"text":"throws","kind":"keyword"},{"text":" -> ","kind":"text"},{"text":"AuthenticateResponse","preciseIdentifier":"s:10StytchCore20AuthenticateResponsea","kind":"typeIdentifier"}]},"01-add-biometrics-05.swift":{"syntax":"swift","fileType":"swift","type":"file","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","        })","        present(alertController, animated: true)","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"identifier":"01-add-biometrics-05.swift","highlights":[{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30}],"fileName":"AuthenticationViewController.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually":{"type":"topic","role":"project","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually","kind":"project","title":"Manage Sessions manually","estimatedTime":"5min","abstract":[{"text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation.","type":"text"}],"url":"\/tutorials\/stytchcore\/manage-sessions-manually"}}}