tch-ios.StytchCore\/documentation\/StytchCore\/StytchClient\/Biometrics-swift.struct\/authenticate(parameters:)-8ycmb","title":"authenticate(parameters:)","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"authenticate","kind":"identifier"},{"text":"(","kind":"text"},{"text":"parameters","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"AuthenticateParameters","kind":"typeIdentifier","preciseIdentifier":"s:10StytchCore0A6ClientV10BiometricsV22AuthenticateParametersV"},{"text":") ","kind":"text"},{"text":"async","kind":"keyword"},{"text":" ","kind":"text"},{"text":"throws","kind":"keyword"},{"text":" -> ","kind":"text"},{"text":"AuthenticateResponse","kind":"typeIdentifier","preciseIdentifier":"s:10StytchCore20AuthenticateResponsea"}],"url":"\/documentation\/stytchcore\/stytchclient\/biometrics-swift.struct\/authenticate(parameters:)-8ycmb","kind":"symbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml#Send-an-EML":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml#Send-an-EML","abstract":[{"text":"Email Magic Links are a great, user-friendly way to get started using Stytch!","type":"text"}],"kind":"section","title":"Send an EML","type":"section","url":"\/tutorials\/stytchcore\/add-eml#Send-an-EML","role":"pseudoSymbol"},"01-add-biometrics-06.swift":{"highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}],"fileType":"swift","fileName":"AuthenticationViewController.swift","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift","identifier":"01-add-biometrics-06.swift","type":"file"},"01-add-biometrics-05.swift":{"type":"file","syntax":"swift","identifier":"01-add-biometrics-05.swift","fileName":"AuthenticationViewController.swift","fileType":"swift","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","        })","        present(alertController, animated: true)","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"highlights":[{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30}]},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient":{"role":"symbol","url":"\/documentation\/stytchcore\/stytchclient","abstract":[{"type":"text","text":"The entrypoint for all Stytch-related interaction."}],"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","title":"StytchClient","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"StytchClient","kind":"identifier"}],"navigatorTitle":[{"text":"StytchClient","kind":"identifier"}],"kind":"symbol","type":"topic"},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Using-more-auth-methods":{"title":"Using more auth methods","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Stytch-Basics\/Using-more-auth-methods","type":"topic","url":"\/tutorials\/stytch\/stytch-basics\/using-more-auth-methods","role":"article","kind":"article","abstract":[]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header#Set-up-a-Request-Interceptor":{"role":"pseudoSymbol","title":"Set up a Request Interceptor","type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","abstract":[{"type":"text","text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current user’s authentication status. This tutorial will show you how you can do this for your networking c