lert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","        })","        present(alertController, animated: true)","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"fileType":"swift","type":"file","identifier":"01-add-biometrics-05.swift","syntax":"swift","highlights":[{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30}],"fileName":"AuthenticationViewController.swift"},"doc://stytch-ios.StytchCore/tutorials/stytch/Intermediate-Stytch-Usage/Going-deeper":{"abstract":[],"kind":"article","title":"Going deeper","role":"article","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Intermediate-Stytch-Usage\/Going-deeper","url":"\/tutorials\/stytch\/intermediate-stytch-usage\/going-deeper","type":"topic"},"doc://stytch-ios.StytchCore/tutorials/stytch/Advanced-Stytch/Pushing-the-limits":{"title":"Pushing the limits","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Advanced-Stytch\/Pushing-the-limits","type":"topic","kind":"article","role