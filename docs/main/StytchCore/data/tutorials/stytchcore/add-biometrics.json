ak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"fileType":"swift","type":"file","syntax":"swift","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}]},"doc://stytch-ios.StytchCore/tutorials/stytch/Intermediate-Stytch-Usage/Going-deeper":{"kind":"article","role":"article","abstract":[],"type":"topic","title":"Going deeper","url":"\/tutorials\/stytch\/intermediate-stytch-usage\/going-deeper","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Intermediate-Stytch-Usage\/Going-deeper"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oaut