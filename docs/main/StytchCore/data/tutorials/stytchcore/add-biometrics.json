    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift","fileType":"swift","identifier":"01-add-biometrics-06.swift","type":"file","fileName":"AuthenticationViewController.swift","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}]},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Getting-started":{"kind":"article","abstract":[],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Stytch-Basics\/Getting-started","role":"article","title":"Getting started","type":"topic","url":"\/tutorials\/stytch\/stytch-basics\/getting-started"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"title":"Configure the StytchClient","url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient","role":"pseudoSymbol","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient"},{"type":"text","text":"."}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","kind":"section","type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor":{"role":"pseudoSymbol","type":"section","title":"Utilizing a just-in-time second factor","abstract":[{"text":"If your app has functionality which requires higher levels of security on certain screens vs others, you may want to consider using step-up authentication or MFA. This tutorial will teach you a simple way of doing this wit