"identifier":"01-add-biometrics-01.swift","syntax":"swift","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    override func viewDidLoad() {","        super.viewDidLoad()","        ...","    }","}"],"type":"file"},"01-add-biometrics-06.swift":{"identifier":"01-add-biometrics-06.swift","fileType":"swift","type":"file","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}],"fileName":"AuthenticationViewController.swift","syntax":"swift","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup":{"role":"project","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup","kind":"project","abstract":[{"text":"This tutorial will walk you through the initial process for setting up the ","type":"text"},{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","isActive":true,"type":"reference"},{"text":".","type":"text"}],"title":"Initial setup","estimatedTime":"5min","type":"topic","url":"\/tutorials\/stytchcore\/initial-setup"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Add-Stytch-to-your-project":{"abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"type":"reference","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","isActive":true},{"type":"text","text":"