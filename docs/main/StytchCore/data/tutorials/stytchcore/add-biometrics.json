 Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift","identifier":"01-add-biometrics-04.swift","type":"file","highlights":[{"line":11},{"line":12},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30},{"line":31},{"line":32},{"line":33}]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Add-Stytch-to-your-project":{"type":"section","title":"Add Stytch to your project.","kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Add-Stytch-to-your-project","role":"pseudoSymbol","url":"\/tutorials\/stytchcore\/initial-setup#Add-Stytch-to-your-project","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient"},{"type":"