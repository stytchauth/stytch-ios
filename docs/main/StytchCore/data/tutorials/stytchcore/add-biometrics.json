on(.init(title: \"OK\", style: .default) { [weak self] _ in","        })","        present(alertController, animated: true)","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"type":"file","highlights":[{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30}],"fileType":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics#Register-and-authenticate-with-Biometrics":{"type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics#Register-and-authenticate-with-Biometrics","kind":"section","url":"\/tutorials\/stytchcore\/add-biometrics#Register-and-authenticate-with-Biometrics","abstract":[{"type":"text","text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!"}],"title":"Register and authenticate with Biometrics","role":"pseudoSymbol"},"01-add-biometrics-01.swift":{"highlights":[],"fileName":"AuthenticationViewController.swift","syntax":"swift","identifier":"01-add-biometrics-01.swift