rics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"identifier":"01-add-biometrics-05.swift","fileName":"AuthenticationViewController.swift","highlights":[{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30}],"fileType":"swift","type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics":{"title":"Add Biometrics","kind":"project","role":"project","estimatedTime":"5min","type":"topic","abstract":[{"text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!","type":"text"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics","url":"\/tutorials\/stytchcore\/add-biometrics"},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient":{"url":"\/documentation\/stytchcore\/stytchclient","type":"topic","navigatorTitle":[{"text":"StytchClient","kind":"identifier"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","title":"StytchClient","abstract":[{"text":"The entrypoint for all Stytch-related interaction.","type":"text"}],"kind":"symbol","role":"symbol","fragments":[{"text":