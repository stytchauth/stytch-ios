       }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"highlights":[{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28},{"line":29},{"line":30}],"fileName":"AuthenticationViewController.swift","type":"file","fileType":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Add-Stytch-to-your-project":{"kind":"section","title":"Add Stytch to your project.","url":"\/tutorials\/stytchcore\/initial-setup#Add-Stytch-to-your-project","abstract":[{"text":"This tutorial will walk you through the initial process for setting up the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient"},{"text":".","type":"text"}],"type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Add-Stytch-to-your-project","role":"pseudoSymbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header#Set-up-a-Request-Interceptor":{"title":"Set up a Request Interceptor","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","type":"section","kind":"section","abstract":[{"type":"text","text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current user’s authentication status. This tutorial will show you how you can do this for your networking client."}],"role":"pseudoSymbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user":{"title":"Manage the curre