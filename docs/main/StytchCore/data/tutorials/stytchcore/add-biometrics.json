== nil else {","            return false","        }","        return true","    }","}"],"highlights":[{"line":7},{"line":8},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22}]},"01-add-biometrics-02.swift":{"content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    override func viewDidLoad() {","        super.viewDidLoad()","        ...","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift","fileType":"swift","fileName":"AuthenticationViewController.swift","highlights":[{"line":11},{"line":12},{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18}],"identifier":"01-add-biometrics-02.swift","type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually#Update-Session-tokens-using-updatesessionTokens":{"url":"\/tutorials\/stytchcore\/manage-sessions-manually#Update-Session-tokens-using-updatesessionTokens","type":"section","title":"Update Session tokens using update(sessionTokens:)","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually#Update-Session-tokens-using-updatesessionTokens","kind":"section","abstract":[{"text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation.","type":"text"}],"role":"pseudoSymbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Add-Stytch-to-your-project":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Add-Stytch-to-your-project","url":"\/tutorials\/stytchcore\/initial-setup#Add-Stytch-to-your-project","role":"pseudoSymbol","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient"},{"type":"text","text":"."}],"kind":"section","title":"Add Stytch to your project.","type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header#Set-up-a-Request-Interceptor":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","abstract":[{"type":"text","text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current user’s authentication status. This tutorial will show you how you can do this for your networking client."}],"kind":"section","title":"Set up a Request Interceptor","type":"section","url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","role":"pseudoSymbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oauth#Add-Third-Party-Identity-Providers":{"role":"pseudoSymbol","abstract":[{"text":"Using OAuth, or social logins, are a familiar and low-friction way to authenticate your users. Learn how to add them in this tutorial!","type":"text"}],"title":"Add Third Party Identity Providers","url":"\/tutorials\/stytchcore\/add-oauth#Add-Third-Party-Identity-Providers","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-oauth#Add-Third-Party-Identity-Providers","type":"section","kind":"section"},"01-add-biometrics-06.swift":{"syntax":"swift","fileType":"swift","identifier":"01-add-biometrics-06.swift","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}],"fileName":"AuthenticationViewController.swift","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oauth#Add-Sign-In-With-Apple":{"role":"pseudoSymbol","kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-oauth#Add-Sign-In-With-Apple","url":"\/tutorials\/stytchcore\/add-oauth#Add-Sign-In-With-Apple","abstract":[{"type":"text","text":"Using OAuth, or social logins, are a familiar and low-friction way to authenticate your users. Learn how to add them in this tutorial!"}],"title":"Add Sign In With Apple","type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually#Update-Session-tokens-via-cookies":{"type":"section","abstract":[{"type":"text","text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation."}],"url":"\/tutorials\/stytchcore\/manage-sessions-manually#Update-Session-tokens-via-cookies","title":"Update Session tokens via cookies","role":"pseudoSymbol","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually#Update-Session-tokens-via-cookies","kind":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml#Set-up-deeplink-handling":{"role":"pseudoSymbol","kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml#Set-up-deeplink-handling","url":"\/tutorials\/stytchcore\/add-eml#Set-up-deeplink-handling","abstract":[{"type":"text","text":"Email Magic Links are a great, user-friendly way to get started using Stytch!"}],"title":"Set up deeplink handling","type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user#Attach-new-factor-to-existing-user":{"role":"pseudoSymbol","kind":"section","abstract":[{"type":"text","text":"This tutorial will teach you a few of the things you can do to manage Stytch users."}],"url":"\/tutorials\/stytchcore\/manage-the-current-user#Attach-new-factor-to-existing-user","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user#Attach-new-factor-to-existing-user","title":"Attach new factor to existing user","type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user","type":"topic","url":"\/tutorials\/stytchcore\/manage-the-current-user","kind":"project","title":"Manage the current user","role":"project","estimatedTime":"10min","abstract":[{"text":"This tutorial will teach you a few of the things you can do to manage Stytch users.","type":"text"}]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics#Register-and-authenticate-with-Biometrics":{"title":"Register and authenticate with Biometrics","type":"section","role":"pseudoSymbol","url":"\/tutorials\/stytchcore\/add-biometrics#Register-and-authenticate-with-Biometrics","abstract":[{"type":"text","text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!"}],"kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics#Register-and-authenticate-with-Biometrics"}}}