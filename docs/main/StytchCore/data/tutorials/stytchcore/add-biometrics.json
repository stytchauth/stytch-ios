ntity-Providers"},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient/Biometrics-swift.struct/authenticate(parameters:)-8ycmb":{"fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"authenticate"},{"kind":"text","text":"("},{"kind":"externalParam","text":"parameters"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:10StytchCore0A6ClientV10BiometricsV22AuthenticateParametersV","text":"AuthenticateParameters"},{"kind":"text","text":") "},{"kind":"keyword","text":"async"},{"kind":"text","text":" "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"kind":"typeIdentifier","preciseIdentifier":"s:10StytchCore20AuthenticateResponsea","text":"AuthenticateResponse"}],"abstract":[{"type":"text","text":"If a valid biometric registration exists, this method confirms the current device owner via the device’s built-in biometric reader and returns an updated session object by either starting a new session or adding a the biometric factor to an existing session."}],"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient\/Biometrics-swift.struct\/authenticate(parameters:)-8ycmb","title":"authenticate(parameters:)","type":"topic","kind":"symbol","url":"\/documentation\/stytchcore\/stytchclient\/biometrics-swift.struct\/authenticate(parameters:)-8ycmb","role":"symbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI":{"url":"\/tutorials\/stytchcore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","abstract":[{"text":"Stytch provides a combine publisher to enable you to reactively set up or tear down your UI when a user logs in or out of your app.","type":"text"}],"role":"pseudoSymbol","type":"section","kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","title":"Dynamically rebuild your UI"},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient/Biometrics-swift.struct/register(parameters:)-m8w7":{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient\/Biometrics-swift.struct\/register(parameters:)-m8w7","title":"register(parameters:)","abstract":[{"text":"When a valid\/active session exists, this method will add a biometric registration for the current user. The user will later be able to start a new session with biometrics or use biometrics as an additional authentication factor.","type":"text"}],"fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"register","kind":"identifier"},{"text":"(","kind":"text"},{"text":"parameters","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"RegisterParameters","kind":"typeIdentifier","preciseIdentifier":"s:10StytchCore0A6ClientV10BiometricsV18RegisterParametersV"},{"text":") ","kind":"text"},{"text":"async","kind":"keyword"},{"text":" ","kind":"text"},{"text":"throws","kind":"keyword"},{"text":" -> ","kind":"text"},{"text":"RegisterCompleteResponse","kind":"typeIdentifier","preciseIdentifier":"s:10StytchCore0A6ClientV10BiometricsV24RegisterCompleteResponsea"}],"url":"\/documentation\/stytchcore\/stytchclient\/biometrics-swift.struct\/register(parameters:)-m8w7","kind":"symbol","type":"topic","role":"symbol"},"01-add-biometrics-06.swift":{"identifier":"01-add-biometrics-06.swift","syntax":"swift","fileName":"AuthenticationViewController.swift","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}],"fileType":"swift","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"type":"file"},"01-add-biometrics-02.swift":{"highlights":[{"line":11},{"line":12},{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18}],"identifier":"01-add-biometrics-02.swift","syntax":"swift","fileType":"swift","fileName":"AuthenticationViewController.swift","content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    override func viewDidLoad() {","        super.viewDidLoad()","        ...","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"type":"file"},"01-add-biometrics-01.swift":{"identifier":"01-add-biometrics-01.swift","syntax":"swift","type":"file","fileName":"AuthenticationViewController.swift","highlights":[],"content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    override func viewDidLoad() {","        super.viewDidLoad()","        ...","    }","}"],"fileType":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor":{"abstract":[{"type":"text","text":"If your app has functionality which requires higher levels of security on certain screens vs others, you may want to consider using step-up authentication or MFA. This tutorial will teach you a simple way of doing this with Stytch."}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor","role":"pseudoSymbol","type":"section","url":"\/tutorials\/stytchcore\/step-up-auth-mfa#Utilizing-a-just-in-t