ine":10},{"line":11}],"syntax":"swift","fileType":"swift","type":"file","fileName":"UserSettingsViewController.swift","content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private func attachToUser(email: String) {","        Task {","            do {","                _ = try await StytchClient.magicLinks.email.send(parameters: .init(email: email))","                showAlertToCheckEmail()","            } catch {}","        }","    }","}"],"identifier":"03-manage-the-current-user-03.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header#Set-up-a-Request-Interceptor":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","role":"pseudoSymbol","type":"section","kind":"section","url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","title":"Set up a Request Interceptor","abstract":[{"text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current user’s authentication status. This tutorial will show you how you can do this for your networking client.","type":"text"}]},"02-manage-the-current-user-04.swift":{"identifier":"02-manage-the-current-user-04.swift","syntax":"swift","content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private var user: User?","","    private func deleteFactor(section: Int, index: Int) {","        guard let user else { return }","","        let factor: AuthenticationFactor","","        \/\/ Emails are displayed in section 0 and phone numbers are displayed in section 1","        switch (section, index) {","        case (0, _):","            factor = .email(user.emails[index].id)","        case (1, _):","            factor = .phoneNumber(user.phoneNumbers[index].id)","        default:","            fatalError(\"We know we don't have a