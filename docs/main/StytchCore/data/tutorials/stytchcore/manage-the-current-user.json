ift","syntax":"swift","content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private var user: User?","","    private func deleteFactor(section: Int, index: Int) {","        guard let user else { return }","","        let factor: AuthenticationFactor","","        \/\/ Emails are displayed in section 0 and phone numbers are displayed in section 1","        switch (section, index) {","        case (0, _):","            factor = .email(user.emails[index].id)","        case (1, _):","            factor = .phoneNumber(user.phoneNumbers[index].id)","        default:","            fatalError(\"We know we don't have a third section\")","        }","","        Task {","            do {","                let response = try await StytchClient.user.deleteFactor(factor)","                user = response.user","                updateUI()","            } catch {}","        }","    }","}"],"type":"file","highlights":[{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28}]},"02-manage-the-current-user-02.swift":{"content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private var user: User?","","    private func deleteFactor(section: Int, index: Int) {","    }","}"],"type":"file","highlights":[{"line":6},{"line":7},{"line":8}],"fileName":"UserSettingsViewController.swift","fileType":"swift","syntax":"swift","identifier":"02-manage-the-current-user-02.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml","kind":"project","estimatedTime":"10min","abstract":[{"text":"Email Magic Links are a great, user-friendly way to get started using Stytch!","type":"text"}],"url":"\/tutorials\/stytchcore\/add-eml","title":"Add Email Magic Links","type":"topic","role":"project"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics":{"url":"\/