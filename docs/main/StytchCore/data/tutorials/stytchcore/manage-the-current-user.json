k {","                do {","                    let response = try await StytchClient.user.get()","                    nameLabel.text = response.user.formattedName","                } catch {}","            }","        }","","        ...","    }","}","","private extension User {","    var formattedName: String {","        [name.firstName, name.lastName]","            .compactMap { $0 }","            .joined(separator: \" \")","    }","}"],"type":"file"},"02-manage-the-current-user-01.swift":{"syntax":"swift","content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private var user: User?","}"],"type":"file","identifier":"02-manage-the-current-user-01.swift","highlights":[],"fileType":"swift","fileName":"UserSettingsViewController.swift"},"doc://stytch-ios.StytchCore/tutorials/stytch/Intermediate-Stytch-Usage/Going-deeper":{"title":"Going deeper","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Intermediate-Stytch-Usage\/Going-deeper","type":"topic","url":"\/tutorials\/stytch\/intermediate-stytch-usage\/going-deeper","abstract":[],"kind":"article","role":"article"},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient/UserManagement":{"abstract":[{"type":"text","text":"The SDK allows you to manage the current userâ€™s information, such as fetching the user, viewing the most recent cached version of the user, or deleting existing authentication factors associated with this user."}],"url":"\/documentation\/stytchcore\/stytchclient\/usermanagement","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient\/UserManagement","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"UserManagement","kind":"identifier"}],"title":"StytchClient.UserManagement","kind":"symbol","type":"topic","navigatorTitle":[{"text":"UserManagement","kind":"identifier"}]},"doc://stytch-ios.StytchCore/tutorials/stytch/Advanced-Stytch/Pushing