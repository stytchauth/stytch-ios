eturn }","","        let factor: AuthenticationFactor","","        \/\/ Emails are displayed in section 0 and phone numbers are displayed in section 1","        switch (section, index) {","        case (0, _):","            factor = .email(user.emails[index].id)","        case (1, _):","            factor = .phoneNumber(user.phoneNumbers[index].id)","        default:","            fatalError(\"We know we don't have a third section\")","        }","","        Task {","            do {","                let response = try await StytchClient.user.deleteFactor(factor)","                user = response.user","                updateUI()","            } catch {}","        }","    }","}"],"highlights":[{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28}],"fileType":"swift","identifier":"02-manage-the-current-user-04.swift","fileName":"UserSettingsViewController.swift","syntax":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header#Set-up-a-Request-Interceptor":{"title":"Set up a Request Interceptor","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","role":"pseudoSymbol","abstract":[{"type":"text","text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current userâ€™s authentication status. This tutorial will show you how you can do this for your networking client."}],"kind":"section","type":"section","url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor"},"03-manage-the-current-user-01.swift":{"type":"file","content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","}"],"highlights":[],"fileType":"swift","identifier":"03-manage-the-current-user-01.swift","fileName":"UserSettingsViewController.swift","syntax":"swift"},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Usin