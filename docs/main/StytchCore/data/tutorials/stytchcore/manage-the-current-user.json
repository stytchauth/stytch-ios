        do {","                    let response = try await StytchClient.user.get()","                    nameLabel.text = response.user.formattedName","                } catch {}","            }","        }","","        ...","    }","}","","private extension User {","    var formattedName: String {","        [name.firstName, name.lastName]","            .compactMap { $0 }","            .joined(separator: \" \")","    }","}"],"type":"file"},"02-manage-the-current-user-01.swift":{"syntax":"swift","content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private var user: User?","}"],"type":"file","identifier":"02-manage-the-current-user-01.swift","highlights":[],"fileType":"swift","fileName":"UserSettingsViewController.swift"},"doc://stytch-ios.StytchCore/tutorials/stytch/Intermediate-Stytch-Usage/Going-deeper":{"title":"Going deeper","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Intermediate-Stytch-Usage\/Going-deeper","type":"topic","url":"\/tutorials\/stytch\/intermediate-stytch-usage\/going-deeper","abstract":[],"kind":"article","role":"article"},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient/UserManagement":{"abstract":[{"type":"text","text":"The SDK allows you to manage the current user’s information, such as fetching the user, viewing the most recent cached version of the user, or deleting existing authentication factors associated with this user."}],"url":"\/documentation\/stytchcore\/stytchclient\/usermanagement","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient\/UserManagement","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"UserManagement","kind":"identifier"}],"title":"StytchClient.UserManagement","kind":"symbol","type":"topic","navigatorTitle":[{"text":"UserManagement","kind":"identifier"}]},"doc://stytch-ios.StytchCore/tutorials/stytch/Advanced-Stytch/Pushing-the-limits":{"role":"article","title":"Pushing the limits","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Advanced-Stytch\/Pushing-the-limits","type":"topic","url":"\/tutorials\/stytch\/advanced-stytch\/pushing-the-limits","kind":"article","abstract":[]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml#Send-an-EML":{"url":"\/tutorials\/stytchcore\/add-eml#Send-an-EML","type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml#Send-an-EML","title":"Send an EML","role":"pseudoSymbol","kind":"section","abstract":[{"text":"Email Magic Links are a great, user-friendly way to get started using Stytch!","type":"text"}]},"02-manage-the-current-user-04.swift":{"identifier":"02-manage-the-current-user-04.swift","content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private var user: User?","","    private func deleteFactor(section: Int, index: Int) {","        guard let user else { return }","","        let factor: AuthenticationFactor","","        \/\/ Emails are displayed in section 0 and phone numbers are displayed in section 1","        switch (section, index) {","        case (0, _):","            factor = .email(user.emails[index].id)","        case (1, _):","            factor = .phoneNumber(user.phoneNumbers[index].id)","        default:","            fatalError(\"We know we don't have a third section\")","        }","","        Task {","            do {","                let response = try await StytchClient.user.deleteFactor(factor)","                user = response.user","                updateUI()","            } catch {}","        }","    }","}"],"type":"file","highlights":[{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28}],"syntax":"swift","fileName":"UserSettingsViewController.swift","fileType":"swift"}}}