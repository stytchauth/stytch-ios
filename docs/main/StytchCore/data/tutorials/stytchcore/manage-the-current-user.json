: UIViewController {","    private let nameLabel: UILabel = .init()","","    override func viewDidLoad() {","        super.viewDidLoad()","","        if let user = StytchClient.user.getSync() {","            nameLabel.text = user.formattedName","        } else {","            Task {","                do {","                    let response = try await StytchClient.user.get()","                    nameLabel.text = response.user.formattedName","                } catch {}","            }","        }","","        ...","    }","}","","private extension User {","    var formattedName: String {","        [name.firstName, name.lastName]","            .compactMap { $0 }","            .joined(separator: \" \")","    }","}"]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","role":"pseudoSymbol","url":"\/tutorials\/stytchcore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","kind":"section","title":"Dynamically rebuild your UI","abstract":[{"text":"Stytch provides a combine publisher to enable you to reactively set up or tear down your UI when a user logs in or out of your app.","type":"text"}],"type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","kind":"section","role":"pseudoSymbol","title":"Configure the StytchClient","type":"section","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient"},{"type":"text","text":"."}],"url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Add-Stytch-to-your-project":{"kind":"section","type":"section","url":"\/tutorials\/stytchcore\/initial-setup#Add-Stytch-to-your-project","title":"Add Stytch to your project.","role":"pseudoSymbol","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Add-Stytch-to-your-project","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient"},{"type":"text","text":"."}]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics":{"url":"\/tutorials\/stytchcore\/add-biometrics","type":"topic","kind":"project","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics","estimatedTime":"5min","title":"Add Biometrics","role":"project","abstract":[{"text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!","type":"text"}]},"doc://stytch-ios.StytchCore/documentation/StytchCore":{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore","abstract":[{"type":"text","text":"Provides native iOS access to the Stytch SDK methods for ultimate flexibility."}],"role":"collection","title":"StytchCore","type":"topic","url":"\/documentation\/stytchcore","kind":"symbol"},"02-manage-the-current-user-04.swift":{"fileName":"UserSettingsViewController.swift","highlights":[{"line":21},{"line":22},{"line":23},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28}],"content":["import StytchCore","import UIKit","","final class UserSettingsViewController: UIViewController {","    private var user: User?","","    private func deleteFactor(section: Int, index: Int) {","        guard let user else { return }","","        let factor: AuthenticationFactor","","        \/\/ Emails are displayed in section 0 and phone numbers are displayed in section 1","        switch (section, inde