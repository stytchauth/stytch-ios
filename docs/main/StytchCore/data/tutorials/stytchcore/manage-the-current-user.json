oller {","    private var user: User?","","    private func deleteFactor(section: Int, index: Int) {","        guard let user else { return }","","        let factor: AuthenticationFactor","","        \/\/ Emails are displayed in section 0 and phone numbers are displayed in section 1","        switch (section, index) {","        case (0, _):","            factor = .email(user.emails[index].id)","        case (1, _):","            factor = .phoneNumber(user.phoneNumbers[index].id)","        default:","            fatalError(\"We know we don't have a third section\")","        }","","        Task {","            do {","                let response = try await StytchClient.user.deleteFactor(factor)","                user = response.user","                updateUI()","            } catch {}","        }","    }","}"],"fileName":"UserSettingsViewController.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header#Set-up-a-Request-Interceptor":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","kind":"section","type":"section","title":"Set up a Request Interceptor","url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header#Set-up-a-Request-Interceptor","role":"pseudoSymbol","abstract":[{"text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current user’s authentication status. This tutorial will show you how you can do this for your networking client.","type":"text"}]},"doc://stytch-ios.StytchCore/tutorials/stytch":{"role":"overview","abstract":[{"type":"text","text":"Go from zero to fully featured authentication in your iOS app powered by Stytch"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch","kind":"overview","type":"topic","url":"\/tutorials\/stytch","title":"Stytch Tutorials"}}}