 NetworkingClient {","    var headerProvider: (() -> [String: String])?","","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        headerProvider?().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"identifier":"01-send-tokens-in-auth-header-03.swift","highlights":[{"line":18},{"line":19},{"line":20},{"line":21}],"syntax":"swift","fileName":"NetworkingClient.swift","fileType":"swift","type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user":{"title":"Manage the current user","type":"topic","abstract":[{"type":"text","text":"This tutorial will teach you a few of the things you can do to manage Stytch users."}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user","role":"project","estimatedTime":"10min","kind":"project","url":"\/tutorials\/stytchcore\/manage-the-current-user"},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient":{"title":"StytchClient","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"StytchClient","kind":"identifier"}],"navigatorTitle":[{"text":"StytchClient","kind":"identifier"}],"url":"\/documentation\/stytchcore\/stytchclient","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","type":"topic","abstrac