ate func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"highlights":[],"identifier":"01-send-tokens-in-auth-header-01.swift","fileName":"NetworkingClient.swift","syntax":"swift","type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user#Attach-new-factor-to-existing-user":{"role":"pseudoSymbol","abstract":[{"type":"text","text":"This tutorial will teach you a few of the things you can do to manage Stytch users."}],"url":"\/tutorials\/stytchcore\/manage-the-current-user#Attach-new-factor-to-existing-user","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user#Attach-new-factor-to-existing-user","title":"Attach new factor to existing user","type":"section","kind":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","type":"section","title":"Configure the StytchClient","url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient"},{"type":"text","text":"."}],"role":"pseudoSymbol","kind":"section"},"01-send-tokens-in-auth-header-06.swift":{"identifier":"01-send-tokens-in-auth-header-06.swift","fileType":"swift","highlights":[{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19}],"content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = {","            var headers: [String: String] = [:]","            if let token = StytchClient.sessions.sessionToken {","                headers[\"Authorization\"] = \"Bearer \\(token)\"","            }","            return headers","        }","        return true","    }","}"],"fileName":"AppDelegate.swift","type":"file","syntax":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user":{"role":"project","abstract":[{"text":"This tutorial will teach you a few of the things you can do to manage Stytch users.","type":"text"}],"type":"topic","url":"\/tutorials\/stytchcore\/manage-the-current-user","title":"Manage the current user","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user","kind":"project","estimatedTime":"10min"}}}