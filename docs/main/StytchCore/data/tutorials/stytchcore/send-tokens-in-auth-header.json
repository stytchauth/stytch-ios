,"01-send-tokens-in-auth-header-06.swift":{"content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = {","            var headers: [String: String] = [:]","            if let token = StytchClient.sessions.sessionToken {","                headers[\"Authorization\"] = \"Bearer \\(token)\"","            }","            return headers","        }","        return true","    }","}"],"identifier":"01-send-tokens-in-auth-header-06.swift","highlights":[{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19}],"fileName":"AppDelegate.swift","syntax":"swift","fileType":"swift","type":"file"},"01-send-tokens-in-auth-header-03.swift":{"syntax":"swift","fileType":"swift","content":["import Foundation","","final class NetworkingClient {","    var headerProvider: (() -> [String: String])?","","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        headerProvider?().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","       