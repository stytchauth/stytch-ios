","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        headerProvider?().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"type":"file","fileName":"NetworkingClient.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml":{"kind":"project","type":"topic","abstract":[{"type":"text","text":"Email Magic Links are a great, user-friendly way to get started using Stytch!"}],"url":"\/tutorials\/stytchcore\/add-eml","estimatedTime":"10min","role":"project","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml","title":"Add Email Magic Links"},"doc://stytch-ios.StytchCore/tutorials/stytch/Intermediate-Stytch-Usage/Going-deeper":{"type":"topic","abstract":[],"title":"Going deeper","url":"\/tutorials\/stytch\/intermediate-stytch-usage\/going-deeper","kind":"article","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Intermediate-Stytch-Usage\/Going-deeper","role":"article"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"role":"pseudoSymbol","url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"type":"reference","identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","isActive":true},{"type":"text","text":"."}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","title":"Configure the StytchClient","type":"section","kind":"section"},"01-send-tokens-in-auth-