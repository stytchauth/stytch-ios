 let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"fileName":"NetworkingClient.swift","highlights":[],"type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-eml#Set-up-deeplink-handling":{"kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-eml#Set-up-deeplink-handling","abstract":[{"text":"Email Magic Links are a great, user-friendly way to get started using Stytch!","type":"text"}],"role":"pseudoSymbol","title":"Set up deeplink handling","url":"\/tutorials\/stytchcore\/add-eml#Set-up-deeplink-handling","type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics#Register-and-authenticate-with-Biometrics":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics#Register-and-authenticate-with-Biometrics","role":"pseudoSymbol","type":"section","abstract":[{"text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!","type":"text"}],"kind":"section","title":"Register and authenticate with Biometrics","url":"\/tutorials\/stytchcore\/add-biometrics#Register-and-authenticate-with-Biometrics"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header","kind":"project","title":"Send tokens in the Authorization header","type":"topic","role":"project","abstract":[{"type":"text","text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current user’s authentication status. This tutorial will show you how you can do this for your networking client."}],"url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header","estimatedTime":"5min"},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Using-more-auth-methods":{"title":"Using more auth methods","type":"topic","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Stytch-Basics\/Using-more-auth-methods","kind":"article","abstract":[],"role":"article","url":"\/tutorials\/stytch\/stytch-basics\/using-more-auth-methods"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa":{"url":"\/tutorials\/stytchcore\/step-up-auth-mfa","abstract":[{"text":"If your app has functionality which requires higher levels of security on certain screens vs others, you may want to consider using step-up authentication or MFA. This tutorial will teach you a simple way of doing this with Stytch.","type":"text"}],"role":"project","estimatedTime":"10min","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/step-up-auth-mfa","type":"topic","kind":"project","title":"Step up auth\/MFA"}}}