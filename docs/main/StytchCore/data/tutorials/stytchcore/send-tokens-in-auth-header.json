inal class NetworkingClient {","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"syntax":"swift","type":"file"},"01-send-tokens-in-auth-header-06.swift":{"syntax":"swift","type":"file","highlights":[{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19}],"fileType":"swift","content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = {","            var headers: [String: String] = [:]","            if let token = StytchClient.sessions.sessionToken {","                headers[\"Authorization\"] = \"Bearer \\(token)\"","            }","            return headers","        }","        return true","    }","}"],"identifier":"01-send-tokens-in-auth-header-06.swift","fileName":"AppDelegate.swift"},"doc://stytch-ios.StytchCore/tutorials/stytch/Advanced-Stytch/Pushing-the-limits":{"kind":"article","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Advanced-Stytch\/Pushing-the-limits","abstract":[],"title":"Pushing the limits","role":"article","type":"topic","url":"\/tutorials\/stytch\/advanced-stytch\/pushing-the-limits"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually":{"type":"topic","title":"Manage Sessions manually","role":"project","url":"\/tutorials\/stytchcore\/manage-sessions-manually","abstract":[{"text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation.","type":"text"}],"kind":"project","estimatedTime":"5min","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually"},"01-send-tokens-in-auth-header-04.swift":{"highlights":[],"fileName":"AppDelegate.swift","fileType":"swift","syntax":"swift","content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        return true","    }","}"],"type":"file","identifier":"01-send-tokens-in-auth-header-04.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics#Register-and-authenticate-with-Biometrics":{"type":"section","kind":"section","title":"Register and authenticate with Biometrics","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-biometrics#Register-and-authenticate-with-Biometrics","url":"\/tutorials\/stytchcore\/add-biometrics#Register-and-authenticate-with-Biometrics","role":"pseudoSymbol","abstract":[{"type":"text","text":"Biometrics is one of the most efficient forms of authentication on mobile devices. Learn how to add it to your app here!"}]},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Getting-started":{"type":"topic","url":"\/tutorials\/stytch