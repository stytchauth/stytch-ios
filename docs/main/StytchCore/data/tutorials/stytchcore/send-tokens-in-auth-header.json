ns launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        return true","    }","}"]},"01-send-tokens-in-auth-header-02.swift":{"identifier":"01-send-tokens-in-auth-header-02.swift","fileType":"swift","fileName":"NetworkingClient.swift","highlights":[{"line":4},{"line":5}],"syntax":"swift","type":"file","content":["import Foundation","","final class NetworkingClient {","    var headerProvider: (() -> [String: String])?","","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"]}}}