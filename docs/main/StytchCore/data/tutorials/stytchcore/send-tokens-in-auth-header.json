uest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        headerProvider?().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"fileType":"swift","identifier":"01-send-tokens-in-auth-header-03.swift","type":"file","fileName":"NetworkingClient.swift","highlights":[{"line":18},{"line":19},{"line":20},{"line":21}]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"type":"section","url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient","role":"pseudoSymbol","abstract":[{"text":"This tutorial will walk you through the initial process for setting up the ","type":"text"},{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","type":"reference","isActive":true},{"text":".","type":"text"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","title":"Configure the StytchClient","kind":"section"},"01-send-tokens-in-auth-header-04.swift":{"syntax":"swift","content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        return true","    }","}"],"fileType":"swift","identif