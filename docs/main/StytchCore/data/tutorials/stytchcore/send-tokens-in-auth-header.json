rl: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        headerProvider?().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"syntax":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"abstract":[{"text":"This tutorial will walk you through the initial process for setting up the ","type":"text"},{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","isActive":true,"type":"reference"},{"text":".","type":"text"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient","kind":"section","role":"pseudoSymbol","title":"Configure the StytchClient","type":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually#Update-Session-tokens-via-cookies":{"abstract":[{"text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation.","type":"text"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually#Update-Session-tokens-via-cookies","title":"Update Session tokens via cookies","url":"\/tutorials\/stytchcore\/manage-sessions-manually#Update-Session-tokens-via-cookies","role":"pseudoSymbol","kind":"section","type":"section"},"doc://stytch-ios.StytchCore/tutoria