       StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = {","            var headers: [String: String] = [:]","            if let token = StytchClient.sessions.sessionToken {","                headers[\"Authorization\"] = \"Bearer \\(token)\"","            }","            return headers","        }","        return true","    }","}"],"identifier":"01-send-tokens-in-auth-header-06.swift","syntax":"swift"},"01-send-tokens-in-auth-header-01.swift":{"content":["import Foundation","","final class NetworkingClient {","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"fileType":"swift","type":"file","syntax":"swift","fileName":"NetworkingClient.swift","highlights":[],"identifier":"01-send-tokens-in-auth-header-01.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oauth#Add-Sign-In-With-Apple":{"url":"\/tutorials\/stytchcore\/add-oauth#Add-Sign-In-With-Apple","title":"Add Sign In With Apple","role":"pseudoSymbol","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-oauth#Add-Sign-In-With-Apple","type":"section","abstract":[{"text":"Using OAuth, or social logins, are a familiar and low-friction way to authenticate your users. Learn how to add them in this tutorial!","type":"text"}],"kind":"section"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa#Confirm-the-one-time