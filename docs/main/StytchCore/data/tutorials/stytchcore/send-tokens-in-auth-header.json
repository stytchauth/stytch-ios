,"final class NetworkingClient {","    private let session = URLSession(configuration: .default)","","    func performRequest(_ method: Method, url: URL) async throws -> (Data, URLResponse) {","        let request = urlRequest(url: url, method: method)","        return try await session.data(for: request)","    }","","    private func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"highlights":[]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor":{"role":"pseudoSymbol","kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor","abstract":[{"type":"text","text":"If your app has functionality which requires higher levels of security on certain screens vs others, you may want to consider using step-up authentication or MFA. This tutorial will teach you a simple way of doing this with Stytch."}],"url":"\/tutorials\/stytchcore\/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor","type":"section","title":"Utilizing a just-in-time second factor"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user","kind":"project","title":"Manage the current user","role":"project","url":"\/tutorials\/stytchcore\/manage-the-current-user","abstract":[{"text":"This tutorial will teach you a few of the things you can do to manage Stytch users.","type":"text"}],"type":"topic","estimatedTime":"10min"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually#Update-Session-tokens-using-updatesessio