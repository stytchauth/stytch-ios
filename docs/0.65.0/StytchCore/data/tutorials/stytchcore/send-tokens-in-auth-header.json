vate func urlRequest(url: URL, method: Method) -> URLRequest {","        var request: URLRequest = .init(url: url)","","        request.httpMethod = method.stringValue","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"type":"file","highlights":[],"fileName":"NetworkingClient.swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-sessions-manually#Update-Session-tokens-via-cookies":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually#Update-Session-tokens-via-cookies","role":"pseudoSymbol","abstract":[{"text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation.","type":"text"}],"type":"section","title":"Update Session tokens via cookies","kind":"section","url":"\/tutorials\/stytchcore\/manage-sessions-manually#Update-Session-tokens-via-cookies"},"01-send-tokens-in-auth-header-05.swift":{"syntax":"swift","identifier":"01-send-tokens-in-auth-header-05.swift","fileType":"swift","fileName":"AppDelegate.swift","highlights":[{"line":13}],"content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = { [:] }","        return true","    }","}"],"type":"file"},"01-send-tokens-in-auth-header-03.swift":{"identifier":"01-send-tokens-in-auth-header-03.swift","syntax":"swift","fileType":"swift","content":["import Foundation","","final class NetworkingClient {","    var headerP