sion-tokens-via-cookies":{"abstract":[{"type":"text","text":"If the out-of-the-box sessions product doesn’t work for your needs, there are options available for manual intervention and manipulation."}],"kind":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-sessions-manually#Update-Session-tokens-via-cookies","title":"Update Session tokens via cookies","type":"section","role":"pseudoSymbol","url":"\/tutorials\/stytchcore\/manage-sessions-manually#Update-Session-tokens-via-cookies"},"doc://stytch-ios.StytchCore/tutorials/stytch/Intermediate-Stytch-Usage/Going-deeper":{"kind":"article","title":"Going deeper","type":"topic","abstract":[],"url":"\/tutorials\/stytch\/intermediate-stytch-usage\/going-deeper","role":"article","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Intermediate-Stytch-Usage\/Going-deeper"},"01-add-biometrics-03.swift":{"content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","        ...","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift","type":"file","identifier":"01-add-biometrics-03.swift","highlights":[{"line":7},{"line":8},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22}],"fileName":"AuthenticationViewController.swift","fileType":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oauth":{"estimatedTime":"5min","kind":"project","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-oauth","abstract":[{"text":"Using OAuth, or social logins, are a familiar and low-friction way to authenticate your users. Learn how to add them in this tutorial!","type":"text"}],"role":"project","url":"\/tutorials\/stytchcore\/add-oauth","type":"topic","title":"Add OAuth"},"01-add-biometrics-06.swift":{"content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}],"fileName":"AuthenticationViewController.swift","syntax":"swift","identifier":"01-add-biometrics-06.swift","fileType":"swift","type":"file"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/send-tokens-in-auth-header":{"role":"project","abstract":[{"type":"text","text":"Many mobile applications choose to use a Bearer token header instead of cookies to inform their backend of the current user’s authentication status. This tutorial will show you how you can do this for your networking client."}],"title":"Send tokens in the Authorization header","type":"topic","url":"\/tutorials\/stytchcore\/send-tokens-in-auth-header","kind":"project","estimatedTime":"5min","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/send-tokens-in-auth-header"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user#Get-current-user":{"type":"section