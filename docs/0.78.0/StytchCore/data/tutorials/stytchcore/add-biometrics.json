Available else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","type":"section","title":"Dynamically rebuild your UI","kind":"section","url":"\/tutorials\/stytchcore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","abstract":[{"type":"text","text":"Stytch provides a combine publisher to enable you to reactively set up or tear down your UI when a user logs in or out of your app."}],"role":"pseudoSymbol"},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient":{"kind":"symbol","navigatorTitle":[{"text":"StytchClient","kind":"identifier"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","role":"symbol","abstract":[{"type":"text","text":"The entrypoint for all Stytch-related interaction."}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"StytchClient","kind":"identifier"}],"url":"\/documentation\/stytchcore\/stytchclient","type":"topic","title":"StytchClient"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa#Confirm-the-one-time-code":{"title":"Confirm the one-time-code","role":"pseudoSymbol","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/step-up-auth-mfa#Confirm-the-one-time-code","kind":"section","type":"section","url":"\/tutorials\/stytchcore\/step-up-auth-mfa#Confirm-the-one-time-code","abstract":[{"text":"If your app has functionality which requires higher levels of security on certain screens vs others, you may want to consider using step-up authentication or MFA. This tutorial will teach you a simple way of doing this with Stytch.","type":"text"}]},"01-add-biometrics-06.swift":{"content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift","fileName":"AuthenticationViewController.swift","fileType":"swift","identifier":"01-add-biometrics-06.swift","type":"file","highlights":[{"line":27},{"line":32},{"line":33},{"line":34},{"line":35},{"line":36},{"line":37},{"line":38},{"line":39},{"line":40}]},"doc://stytch-ios.StytchCore/documentation/StytchCore/StytchClient/Biometrics-swift.struct/register(parameters:)-m8w7":{"fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"register"},{"kind":"text","text":"("},{"kind":"externalParam","text":"parameters"},{"kind":"text","text":": "},{"preciseIdentifier":"s:10StytchCore0A6ClientV10BiometricsV18RegisterParametersV","kind":"typeIdentifier","text":"RegisterParameters"},{"kind":"text","text":") "},{"kind":"keyword","text":"async"},{"kind":"text","text":" "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"preciseIdentifier":"s:10StytchCore0A6ClientV10BiometricsV24RegisterCompleteResponsea","kind":"t