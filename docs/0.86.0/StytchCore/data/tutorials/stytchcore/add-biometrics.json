it","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"syntax":"swift"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor","title":"Utilizing a just-in-time second factor","url":"\/tutorials\/stytchcore\/step-up-auth-mfa#Utilizing-a-just-in-time-second-factor","type":"section","role":"pseudoSymbol","abstract":[{"type":"text","text":"If your app has functionality which requires higher levels of security on certain screens vs others, you may want to consider using step-up authentication or MFA. This tutorial will teach you a simple way of doing this with Stytch."}],"kind":"section"},"01-add-biometrics-03.swift":{"syntax":"swift","type":"file","fileName":"AuthenticationViewController.swift","fileType":"swift","highlights":[{"line":7},{"line":8},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22}],"content":["import StytchCore","import UIKit","","final class AuthenticationViewController: UIViewController {","    var onAuthenticate: ((AuthenticateResponseType) -> Void)?","","    private let biometricsButton: UIButton","","    override func viewDidLoad() {","        super.viewDidLoad()","        ...","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"],"identifier":"01-add-biometrics-03.swift"},"doc://stytch-ios.StytchCore/tutorials/stytch":{"abstract":[{"text":"Go from zero to fully featured authentication in your iOS app powered by Stytch","type":"text"}],"type":"topic","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch","title":"Stytch Tutorials","url":"\/tutorials\/stytch","kind":"overview","role":"overview"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-oauth":{"kind":"project","title":"Add OAuth","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/add-oauth","type":"topic","abstract":[{"text":"Using OAuth, or social logins, are a familiar and low-friction way to authenticate your users. Learn how to add them in this tutorial!","type":"text"}],"estimatedTime":"5min","role":"project","url":"\/tutorials\/stytchcore\/add-oauth"},"doc://stytch-ios.StytchCore/tutorials/stytch/Stytch-Basics/Getting-started":{"title":"Getting started","url":"\/tutorials\/stytch\/stytch-basics\/getting-started","role":"article","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Stytch-Basics\/Getting-started","type":"topic","abstract":[],"kind":"article"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","isActive":true,"type":"reference"},{"type":"text","text":"."}],"kind":"section","title":"Configure the StytchClient","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient","url":"\/tutorials\/stytchcore\/initial-setup#Conf