().forEach { field, value in","            request.addValue(value, forHTTPHeaderField: field)","        }","","        switch method {","        case .get, .delete:","            break","        case let .post(data), let .put(data):","            request.httpBody = data","        }","","        return request","    }","}"],"type":"file","syntax":"swift","identifier":"01-send-tokens-in-auth-header-03.swift"},"01-send-tokens-in-auth-header-06.swift":{"content":["import StytchCore","import UIKit","","@UIApplicationMain","final class AppDelegate: NSObject, UIApplicationDelegate {","    let networkingClient: NetworkingClient = .init()","","    func application(","        _ application: UIApplication,","        didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey : Any]? = nil","    ) -> Bool {","        StytchClient.configure(publicToken: publicToken)","        networkingClient.headerProvider = {","            var headers: [String: String] = [:]","            if let token = StytchClient.sessions.sessionToken {","                headers[\"Authorization\"] = \"Bearer \\(token)\"","            }","            return headers","        }","        return true","    }","}"],"syntax":"swift","fileName":"AppDelegate.swift","fileType":"swift","identifier":"01-send-tokens-in-auth-header-06.swift","type":"file","highlights":[{"line":13},{"line":14},{"line":15},{"line":16},{"line":17},{"line":18},{"line":19}]},"doc://stytch-ios.StytchCore/tutorials/stytch/Intermediate-Stytch-Usage/Going-deeper":{"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/stytch\/Intermediate-Stytch-Usage\/Going-deeper","url":"\/tutorials\/stytch\/intermediate-stytch-usage\/going-deeper","type":"topic","abstract":[],"kind":"article","title":"Going deeper","role":"article"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/step-up-auth-mfa#Confirm-the-one-time-code":{"role":"pseudoSymbol","title":"Confirm the one-time-code","kind":"section","identifier":"doc:\/\/stytch-ios.StytchCo