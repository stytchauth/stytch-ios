,"    override func viewDidLoad() {","        super.viewDidLoad()","","        biometricsButton.addTarget(self, action: #selector(didTapBiometricsButton(sender:)), for: [.touchUpInside])","        ...","    }","","    private func handleAuthentication(response: AuthenticateResponseType) {","        if canPerformBiometricCheck(), !StytchClient.biometrics.registrationAvailable {","            promptForBiometricsRegistration(user: response.user)","        }","        onAuthenticate(response)","    }","","    private func promptForBiometricRegistration(user: User) {","        let message = \"Register for Biometrics!\"","        let alertController = UIAlertController(title: nil, message: message, preferredStyle: .alert)","        alertController.addAction(.init(title: \"OK\", style: .default) { [weak self] _ in","            self?.registerBiometrics(identifier: user.emails.first?.email ?? user.id.rawValue)","        })","        present(alertController, animated: true)","    }","","    private func registerBiometrics(identifier: String) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.register(parameters: .init(identifier: identifier))","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func updateBiometricsButtonHidden() {","        guard canPerformBiometricCheck(), StytchClient.biometrics.registrationAvailable else {","            biometricsButton.isHidden = true","            return","        }","","        biometricsButton.isHidden = false","    }","","    @objc private func didTapBiometricsButton(sender: UIButton) {","        Task {","            do {","                let authResponse = try await StytchClient.biometrics.authenticate(parameters: .init())","                handleAuthentication(response: authResponse)","            } catch {}","        }","    }","","    private func canPerformBiometricCheck() -> Bool {","        var error: NSError?","        guard LAContext().canEvaluatePolicy(.deviceOwnerAuthenticationWithBiometrics, error: &error), error == nil else {","            return false","        }","        return true","    }","}"]},"doc://stytch-ios.StytchCore/tutorials/StytchCore/manage-the-current-user#Get-current-user":{"url":"\/tutorials\/stytchcore\/manage-the-current-user#Get-current-user","kind":"section","type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/manage-the-current-user#Get-current-user","abstract":[{"text":"This tutorial will teach you a few of the things you can do to manage Stytch users.","type":"text"}],"title":"Get current user","role":"pseudoSymbol"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI":{"abstract":[{"text":"Stytch provides a combine publisher to enable you to reactively set up or tear down your UI when a user logs in or out of your app.","type":"text"}],"identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","type":"section","kind":"section","role":"pseudoSymbol","url":"\/tutorials\/stytchcore\/rebuild-ui-on-auth-change#Dynamically-rebuild-your-UI","title":"Dynamically rebuild your UI"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/initial-setup#Configure-the-StytchClient":{"role":"pseudoSymbol","title":"Configure the StytchClient","abstract":[{"type":"text","text":"This tutorial will walk you through the initial process for setting up the "},{"identifier":"doc:\/\/stytch-ios.StytchCore\/documentation\/StytchCore\/StytchClient","type":"reference","isActive":true},{"type":"text","text":"."}],"kind":"section","url":"\/tutorials\/stytchcore\/initial-setup#Configure-the-StytchClient","type":"section","identifier":"doc:\/\/stytch-ios.StytchCore\/tutorials\/StytchCore\/initial-setup#Configure-the-StytchClient"},"doc://stytch-ios.StytchCore/tutorials/StytchCore/add-biometrics":{"abstract":[{"type":